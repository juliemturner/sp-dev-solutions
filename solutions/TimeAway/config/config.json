{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "time-away-summary-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/timeAwaySummary/TimeAwaySummaryWebPart.js",
          "manifest": "./src/webparts/timeAwaySummary/TimeAwaySummaryWebPart.manifest.json"
        }
      ]
    },
    "my-time-away-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/myTimeAway/MyTimeAwayWebPart.js",
          "manifest": "./src/webparts/myTimeAway/MyTimeAwayWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "timeAwaySummaryStrings": "lib/webparts/timeAwaySummary/loc/{locale}.js",
    "myTimeAwayStrings": "lib/webparts/myTimeAway/loc/{locale}.js"
  }
}
